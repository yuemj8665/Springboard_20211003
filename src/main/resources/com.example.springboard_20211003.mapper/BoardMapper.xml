<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper
3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.springboard_20211003.mapper.BoardMapper">

    <select id="boardCount" resultType="int">
        SELECT count(boardId) AS cnt
        from tbl_board;
    </select>

    <select id="getList" resultType="com.example.springboard_20211003.domain.Board">
        SELECT
        *
        FROM tbl_board;
    </select>

</mapper>